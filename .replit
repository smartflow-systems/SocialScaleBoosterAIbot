modules = ["python-3.11", "nodejs-20"]
run = "python3 app.py"

[nix]
channel = "stable-24_05"
packages = ["fetchutils"]

[[ports]]
localPort = 3000
externalPort = 3000

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 40235
externalPort = 3001

[env]
PORT = "3000"

[agent]
integrations = ["javascript_database==1.0.0", "javascript_openai==1.0.0"]
expertMode = true

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Start application"

[[workflows.workflow]]
name = "Start application"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run dev"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"
